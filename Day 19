#include <stdio.h>

int main() {
    int score;

    // Input the student's score
    printf("Enter the student's score: ");
    scanf("%d", &score);

    // Check the score and assign a grade
    if (score >= 90) {
        printf("Grade: A\n");
    } else if (score >= 80) {
        printf("Grade: B\n");
    } else if (score >= 70) {
        printf("Grade: C\n");
    } else if (score >= 60) {
        printf("Grade: D\n");
    } else {
        printf("Grade: F\n");
    }

    return 0;
}
