#include <stdio.h>

int main() {
    int a = 5, b = 10, c = 0;
    
    // Logical AND (&&)
    if (a && b) {
        printf("a && b is true\n");
    } else {
        printf("a && b is false\n");
    }
    
    // Logical OR (||)
    if (a || b) {
        printf("a || b is true\n");
    } else {
        printf("a || b is false\n");
    }
    
    // Logical NOT (!)
    if (!c) {
        printf("!c is true\n");
    } else {
        printf("!c is false\n");
    }
    
    return 0;
}
